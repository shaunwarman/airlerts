    "test": "npm run lint && npm run test:client && npm run test:server",
    "build:client": "NODE_ENV=production webpack",
    "build:server": "NODE_ENV=production gulp build",
    "build": "npm run build:client && npm run build:server",
    "start:client": "NODE_ENV=development hjs-dev-server",
    "start:server": "NODE_ENV=development npm run build:server && gulp start",
    "start": "npm run start:client & npm run start:server",
    "deploy": "npm run lint && npm run test && npm run build | echo \"no deployment script specified\""
